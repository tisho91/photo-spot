{
  "name": "photo-spot",
  "homepage": ".",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "workspaces": [
    "client/",
    "server/"
  ],
  "scripts": {
    "client": "yarn workspace @photo-spot/client start",
    "server": "yarn workspace @photo-spot/server serve",
    "server:prod": "yarn workspace @photo-spot/server start",
    "build-client": "yarn workspace @photo-spot/client build",
    "start": "yarn workspace @photo-spot/server start",
    "start:dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client \"",
    "postinstall": "yarn workspace @photo-spot/server build",
    "heroku-postbuild": "yarn build-app"
  },
  "dependencies": {
    "concurrently": "^6.0.2"
  },
  "engines": {
    "node": "16.13.0"
  },
  "author": "",
  "license": "ISC",
  "packageManager": "yarn@3.1.1"
}
